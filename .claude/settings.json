{
  "includeCoAuthoredBy": false,
  "permissions": {
    "defaultMode": "default",
    "allow": [],
    "deny": [
      "Bash(rm:)",
      "Bash(sudo:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(git push:)",
      "Bash(git rebase:)",
      "Bash(git rm:)",
      "Bash(pnpm publish:*)",
      "Bash(cargo publish:*)",
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(*.local.*)"
    ]
  },
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "600000",
    "BASH_MAX_TIMEOUT_MS": "900000"
  }
}
